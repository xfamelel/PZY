local function FZZI_fake_script() -- OOF.Button5Script 
	local script = Instance.new('LocalScript', OOF)

	local btn = script.Parent
	local uiGradient = btn:WaitForChild("UIGradient")
	
	local isHovering = false
	
	local tweenService = game:GetService("TweenService")
	local tweenInfo = TweenInfo.new(0.4, Enum.EasingStyle.Quint, Enum.EasingDirection.InOut)
	
	local gradientRemoveTween = tweenService:Create(uiGradient, tweenInfo, {Offset = Vector2.new(-1, 0)})
	local gradientRestoreTween = tweenService:Create(uiGradient, tweenInfo, {Offset = Vector2.new(0.4, 0)})
	
	
	btn.MouseEnter:Connect(function()
		
		isHovering = true
		
		gradientRemoveTween:Play()
	end)
	
	btn.MouseLeave:Connect(function()
		
		isHovering = false
		
		gradientRestoreTween:Play()
	end)
	
	btn.MouseButton1Down:Connect(function()
		
		gradientRestoreTween:Play()
	end)
	
	btn.MouseButton1Up:Connect(function()
		
		if not isHovering then
			gradientRestoreTween:Play()
		else
			gradientRemoveTween:Play()
		end
	end)
end
coroutine.wrap(FZZI_fake_script)()

Main.Visible=false
START.Visible=true

START.MouseButton1Down:connect(function()
	Main.Visible=true
	Main.Draggable=true
	START.Visible=false
end)

EXIT.MouseButton1Down:connect(function()
	Main.Visible=false
	Main.Draggable=false
	START.Visible=true
end)

--BUTTONS SCRIPTING--

Ragdoll2.MouseButton1Down:connect(function()
	loadstring(game:HttpGet("https://pastebin.com/raw/NawMZzhW", true))()
end)

Ragdoll1.MouseButton1Down:connect(function()
	loadstring(game:HttpGet(('https://gitlab.com/Tsuniox/lua-stuff/-/raw/master/RagdollEngineGUI.lua'),true))()
end)

OOF.MouseButton1Down:connect(function()
	repeat game:GetService("RunService").Stepped:wait()
		for i,v in pairs(game.Players:GetChildren()) do
			v.Character.Head.Died.TimePosition = .15
			v.Character.Head.Died:Play()
		end
	until game.Players.LocalPlayer.Character.Humanoid.Health == 0
end)
