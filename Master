-- Scripts:

local function RHLN_fake_script() -- Ragdoll2.Button5Script 
	local script = Instance.new('LocalScript', Ragdoll2)

	local btn = script.Parent
	local uiGradient = btn:WaitForChild("UIGradient")
	
	local isHovering = false
	
	local tweenService = game:GetService("TweenService")
	local tweenInfo = TweenInfo.new(0.4, Enum.EasingStyle.Quint, Enum.EasingDirection.InOut)
	
	local gradientRemoveTween = tweenService:Create(uiGradient, tweenInfo, {Offset = Vector2.new(-1, 0)})
	local gradientRestoreTween = tweenService:Create(uiGradient, tweenInfo, {Offset = Vector2.new(0.4, 0)})
	
	
	btn.MouseEnter:Connect(function()
		
		isHovering = true
		
		gradientRemoveTween:Play()
	end)
	
	btn.MouseLeave:Connect(function()
		
		isHovering = false
		
		gradientRestoreTween:Play()
	end)
	
	btn.MouseButton1Down:Connect(function()
		
		gradientRestoreTween:Play()
	end)
	
	btn.MouseButton1Up:Connect(function()
		
		if not isHovering then
			gradientRestoreTween:Play()
		else
			gradientRemoveTween:Play()
		end
	end)
end
coroutine.wrap(RHLN_fake_script)()
local function PQBYXF_fake_script() -- Ragdoll1.Button5Script 
	local script = Instance.new('LocalScript', Ragdoll1)

